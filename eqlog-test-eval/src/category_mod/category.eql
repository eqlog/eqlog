type Obj;
type Morphism;

func dom(Morphism) -> Obj;
func cod(Morphism) -> Obj;
func comp(Morphism, Morphism) -> Morphism;
func id(Obj) -> Morphism;

rule {
    if cod(f) = dom(g);
    then comp(g, f)!;
}
rule {
    if h = comp(_, f);
    if d = dom(f);
    then dom(h) = d;
}
rule {
    if h = comp(g, _);
    if c = cod(g);
    then cod(h) = c;
}
rule {
    if x: Obj;
    if y: Obj;
    then id(x)!;
    then id(y)!;
}
