type Obj;
type Morphism;

func mor_dom(Morphism) -> Obj;
func mor_cod(Morphism) -> Obj;
func comp(Morphism, Morphism) -> Morphism;
func id(Obj) -> Morphism;

rule {
    if mor_cod(f) = mor_dom(g);
    then comp(g, f)!;
}
rule {
    if h = comp(_, f);
    if d = mor_dom(f);
    then mor_dom(h) = d;
}
rule {
    if h = comp(g, _);
    if c = mor_cod(g);
    then mor_cod(h) = c;
}
rule {
    if x: Obj;
    if y: Obj;
    then id(x)!;
    then id(y)!;
}
