model Pointed {
    type P;

    func pt() -> P;

    pred is_subterminal();

    rule pointed_reflexivity_member {
        if x: P;
        then x = x;
    }

    rule is_subterminal_member_collapse {
        if is_subterminal();
        if x: P;
        if y: P;
        then x = y;
    }
}

rule pointed_reflexivity_global {
    if ptd: Pointed;
    if x: ptd.P;
    then x = x;
}

pred is_subterminal_pointed(ptd: Pointed);
rule pointed_collapse_convoluted {
    if ptd_0: Pointed;
    if ptd_1: Pointed;
    if x: ptd_0.P;
    if y: ptd_0.P;
    if is_subterminal_pointed(ptd_0);
    if ptd_0 = ptd_1;
    then x = y;
}
